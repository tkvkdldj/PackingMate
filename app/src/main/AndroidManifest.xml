<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    package="com.example.packingmate">

    <application
        android:allowBackup="true"
        android:dataExtractionRules="@xml/data_extraction_rules"
        android:fullBackupContent="@xml/backup_rules"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.PackingMate"
        tools:targetApi="31">
        <activity
            android:name=".AIListActivity"
            android:exported="false"
            android:theme="@style/Theme.PackingMate" />
        <activity
            android:name=".MainActivity"
            android:exported="true">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity> <!-- 리스트 만들기 화면 -->
        <activity android:name=".InputActivity" /> <!-- 리스트 확인 화면 -->
        <activity android:name=".ListActivity" /> <!-- 대기 화면 -->
        <activity android:name=".LoadingActivity" /> <!-- 상세 화면 -->
        <activity
            android:name=".ListDetailActivity"
            android:exported="true"
            android:parentActivityName=".ListActivity" />
    </application>

</manifest>